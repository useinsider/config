{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "lib": [
            "DOM",
        ],
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "outDir": "dist",
        "baseUrl": ".",
        "types": [
            "vue"
        ],
        "moduleResolution": "Node"
    },
    "include": [
        "./**/*"
    ],
}
