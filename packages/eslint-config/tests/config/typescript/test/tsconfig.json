{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "lib": [
            "ES2022",
        ],
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "outDir": "dist",
        "baseUrl": ".",
        "moduleResolution": "Node",
        "paths": {
            "@useinsider/eslint-config": [
                "../../../dist/main"
            ],
        },
    },
    "include": [
        "./*.ts"
    ],
}
