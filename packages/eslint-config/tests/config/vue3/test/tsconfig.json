{
    "compilerOptions": {
        "allowJs": true,
        "target": "ES2022",
        "module": "ESNext",
        "lib": [
            "DOM",
            "ES2022",
        ],
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "outDir": "dist",
        "baseUrl": ".",
        "types": [
            "vue"
        ],
        "moduleResolution": "Node",
        "paths": {
            "@useinsider/eslint-config": [
                "../../../../dist/main"
            ],
        }
    },
    "include": [
        "./**/*.vue"
    ],
}
